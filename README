




    dom(document).ready(function() {

        dom(find(".myclass"))
                .hide()
                .show()
                .css({
                    color:"red"
                })
                .html("Q")
                .on("click", function(x) {
                    dom(this).css({
                        backgroundColor: "green"
                    });
                });

        dom(find("#myid"))
                .on("click", function(x) {
                    dom(this).opacity(.5);
                });
    });



